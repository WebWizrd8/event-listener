{
    "name": "my-turborepo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "server/*",
        "components/*",
        "ui/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "latest",
        "turbo": "^1.6.3"
    },
    "engines": {
        "node": ">=16.15.0"
    },
    "dependencies": {},
    "packageManager": "yarn@1.22.19"
}
